<div class="room-list">
  <div class="room-header">
    {{#link-to 'room'}}
      Rooms
    {{/link-to}}
    {{#link-to 'contact'}}
      Contacts
    {{/link-to}}
    {{#link-to 'room.create'}}
      Create room
    {{/link-to}}
  </div>

  <ul>
  {{#each model.rooms as |room|}}
    <li>
      {{#link-to 'room.item' room.id}}
        Name: {{room.name}}
      {{/link-to}}
    </li>
  {{/each}}
  </ul>
</div>